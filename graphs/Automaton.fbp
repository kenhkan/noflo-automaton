EXPORT=SETUP.URL:URL
EXPORT=SETUP.RULES:RULES
EXPORT=SETUP.OPTIONS:OPTIONS
EXPORT=RUN.OUT:OUT
EXPORT=SETUP.ERROR:ERROR
 
# The looper
 
Setup(automaton/Setup) OUT -> IN Iterate(automaton/Iterate) OUT -> IN TestActions(automaton/TestActions) OUT -> IN TestEffects(automaton/TestEffects) RETRY -> IN TestActions()
TestEffects() OUT -> IN Iterate()
 
# Various action runners
 
TestActions() ACTION -> IN Click(automaton/Click) OUT -> IN Fill(automaton/Fill) OUT -> IN Extract(automaton/Extract)
 
# Run on exit

Iterate() EXIT -> IN Run(automaton/Run)
TestActions() EXIT -> IN Run()
TestEffects() EXIT -> IN Run()
